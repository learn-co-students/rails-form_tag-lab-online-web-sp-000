<%= form_tag students_path do %>
    <h3>"Student Form"</h3>
    <label>Student First Name:</label><br>
    <%= text_field_tag :'student[first_name]' %>
    
    <label>Student Last Name:</label><br>
    <%= text_field_tag :'student[last_name]' %>

    <%= submit_tag 'Submit Student' %>
<% end %>