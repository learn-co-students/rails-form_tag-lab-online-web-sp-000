<h3>Student Form</h3>
<%= form_tag new_student_path, :method => :get do %>
  <label>First Name:</label><br>
  <%= text_field_tag :'student[first_name]' %><br>
 
  <label>Last Name:</label><br>
  <%= text_area_tag :'student[last_name]' %><br>
  <input type="submit" value="Submit Student">
<% end %>
<%= params.inspect %>