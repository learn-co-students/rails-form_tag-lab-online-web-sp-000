<%= form_tag students_path do %>

    <h1>Student Form</h1>
    <label>First Name:</label>
    <%= text_field_tag name='student[first_name]' %>

    <label>Last Name:</label>
    <%= text_field_tag name='student[last_name]' %>

    <%= submit_tag "Submit Student" %>
    <% if session[:form_params] %>
    <%= session[:form_params] %>
    <% end %>
<% end %>