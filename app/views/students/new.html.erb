<h3>Student Form<h3>

<%=form_tag students_path do %>

<label>Students Firstname:</label><br> 
<%=text_field_tag :'student[first_name]'%><br><br>

<label>Students Lastname:</label>
<%=text_field_tag :'student[last_name]'%> 

<%=hidden_field_tag :authenticity_token, form_authenticity_token%>

<input type="submit" value='Submit Student'>
<%end%>
