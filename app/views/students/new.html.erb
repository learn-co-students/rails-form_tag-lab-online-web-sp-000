<h3>Student Form</h3>

<%= "#{@params[:first_name]} was created!" if @params[:first_name] %>

<%= form_tag students_path do %>
  <label>First Name:</label><br>
  <%= text_field_tag(:'student[first_name]', placeholder: true) %><br>

  <label>Last Name:</label><br>
  <%= text_field_tag(:'student[last_name]', placeholder: true) %><br>

  <%= submit_tag "Submit Student" %>
<% end %>
